  <div class="row">
    <h3>ジャンル編集</h3>
  </div>
  <%= form_with model: @genre, url:admin_genre_path, method: :patch, local: true do |f| %>
  <dvi class="row pt-5">
    <div class="col-md-6">
       <%= f.label :ジャンル名, class: "h5" %>
       <%= f.text_field :name, class: "offset-md-1 h5" %>
       <%= f.submit "変更を保存", class: "offset-md-1 btn btn-success"%>
    </div>
      <% end %>
  </dvi>